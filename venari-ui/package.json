  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "axios": "^0.27.2",
    "marked": "^4.0.18",
    "pinia": "^2.0.16",
    "sass": "^1.54.0",
    "vue": "^3.2.37",
    "vue-dragscroll": "^4.0.3",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/marked": "^4.0.3",
    "@types/node": "^16.11.45",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^20.0.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^13.0.2",
    "typescript": "~4.7.4",
    "vite": "^3.0.1",
    "vitest": "^0.18.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.8"
  }
}
